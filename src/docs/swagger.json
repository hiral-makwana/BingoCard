  "paths": {
    "/v1/private/cards/create": {
      "post": {
        "tags": [
          "Card"
        ],
        "summary": "create card.",
        "description": "This API create card",
        "produces": [
          "multipart/form-data"
        ],
        "consumes":[
          "multipart/form-data"
        ],
        "parameters": [
          {
            "in": "formData",
            "name": "card_image",
            "type": "array",
            "items": {
              "type": "file"
            },
            "required": false,
            "collectionFormat": "multi",
            "description": "The file(s) to upload"
          },
          {
            "in": "formData",
            "name": "card_logo",
            "type": "object",
            "properties": {
              "setting1": {
                "type": "string",
                "example": "value1"
              },
              "setting2": {
                "type": "integer",
                "format": "int32",
                "example": 42
              }
            },
            "required": true,
            "description": "Card logo settings"
          },
          {
            "in": "formData",
            "name": "card_title",
            "type": "string",
            "required": true,
            "description": "Card title"
          },
          {
            "in": "formData",
            "name": "card_type",
            "type": "string",
            "enum": ["text", "images", "emojis", "numbers", "combo"],
            "required": true,
            "description": "Card type"
          },
          {
            "in": "formData",
            "name": "card_grid",
            "type": "integer",
            "format": "int32",
            "required": true,
            "description": "Card grid value"
          },
          {
            "in": "formData",
            "name": "card_settings",
            "type": "object",
            "properties": {
              "setting1": {
                "type": "string",
                "example": "value1"
              },
              "setting2": {
                "type": "integer",
                "format": "int32",
                "example": 42
              }
            },
            "required": true,
            "description": "Card settings"
          },
          {
            "in": "formData",
            "name": "card_items",
            "type": "object",
            "properties": {
              "item1": {
                "type": "string",
                "example": "item1-value"
              },
              "item2": {
                "type": "integer",
                "format": "int32",
                "example": 99
              }
            },
            "required": true,
            "description": "Card items"
          },
          {
            "in": "formData",
            "name": "card_style",
            "type": "object",
            "properties": {
              "style1": {
                "type": "string",
                "example": "style-value"
              },
              "style2": {
                "type": "boolean",
                "example": true
              }
            },
            "required": true,
            "description": "Card style"
          }
        ],
        "responses": {
          "200": {
            "description": "A successful response with a message.",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "Card created successfully!"
                }
              }
            }
          },
          "400": {
            "description": "Something went wrong, please try again."
          },
          "401": {
            "description": "Your card is not created."
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
